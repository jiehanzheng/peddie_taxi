<h1>Listing proposals</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Venue</th>
      <th>Comments</th>
      <th>Leaving at</th>
      <th>Returning at</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @proposals.each do |proposal| %>
    <tr>
      <td><%= proposal.user_id %></td>
      <td><%= proposal.venue_id %></td>
      <td><%= proposal.comments %></td>
      <td><%= proposal.leaving_at %></td>
      <td><%= proposal.returning_at %></td>
      <td><%= link_to 'Show', proposal %></td>
      <td><%= link_to 'Edit', edit_proposal_path(proposal) %></td>
      <td><%= link_to 'Destroy', proposal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Proposal', new_proposal_path %>
